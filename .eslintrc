{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",

  "ecmaFeatures": {
	"experimentalObjectRestSpread": true
  },

  "rules": {
	"semi": [2, "always"],
	"no-unused-vars": 0,
	"no-mixed-spaces-and-tabs": 0,
	"no-console": 0,
	"no-case-declarations": 0
  },

  "env": {
	"browser": true,
	"worker": true,
	"jquery": true,
	"commonjs": true,
	"jasmine" : true
  },

  "globals": {
	"angular": true,
	"inject": true,
	"Map": true,
	"Set": true,
	"Symbol": true
  }

}